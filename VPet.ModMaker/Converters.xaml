<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:c="https://hkw.com/wpf">
  <c:MarginMultiConverter x:Key="MarginConverter" />
  <c:MarginMultiConverter x:Key="RatioMarginConverter" HasRatio="True" />
  <c:DoubleCalculatorMultiConverter x:Key="CalculatorConverter" />
  <c:StringFormatMultiConverter x:Key="StringFormatConverter" ReplaceUnsetValue="null" />
  <c:SolidColorBrushToMediaColorConverter x:Key="SolidColorBrushToMediaColorConverter" />
  <!--<c:MaxConverter x:Key="MaxConverter" />-->
  <c:BoolToVisibilityConverter x:Key="FalseToCollapsedConverter" />
  <c:FirstEqualsSecondMultiConverter x:Key="EqualsConverter" />
  <c:FirstEqualsSecondMultiConverter x:Key="NotEqualsConverter" IsInverted="True" />
  <c:NullToBoolConverter x:Key="NullToFalseConverter" />
  <c:AllBoolToVisibilityMultiConverter
    x:Key="AllTrueToCollapsedConverter"
    DefaultResult="Collapsed"
    FalseValue="Visible"
    TrueValue="Collapsed" />
  <c:BoolInverteConverter x:Key="BoolInverter" />
  <c:EnumToEnumInfoConverter x:Key="EnumToEnumInfoConverter" />
  <c:EnumsToEnumInfosConverter x:Key="EnumsToEnumInfosConverter" />
  <c:Int32CalculatorMultiConverter x:Key="Int32CalculatorMultiConverter" ResultToString="True" />
</ResourceDictionary>